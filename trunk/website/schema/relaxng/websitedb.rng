    <div db:name="appendix">
      <define name="appendix">
        <element name="appendix">
          <ref name="appendix.attlist"/>
          
          
          <ref name="bookcomponent.title.content"/>
          <zeroOrMore>
            <ref name="nav.class"/>
          </zeroOrMore>
          
          <ref name="bookcomponent.content"/>
