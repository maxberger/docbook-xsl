"&lt;answer&gt;&lt;para&gt;...&lt;/para&gt;&lt;/answer&gt;\n"
"&lt;/qandaentry&gt;\n"
"\n"
"&lt;qandaentry&gt;&lt;question&gt;&lt;para&gt;Agfa, Inc.&lt;/para&gt;&lt;/question&gt;\n"
"&lt;answer&gt;&lt;para&gt;...&lt;/para&gt;&lt;/answer&gt;\n"
"&lt;/qandaentry&gt;\n"
"\n"
"&lt;/qandadiv&gt;\n"
"&lt;/qandadiv&gt;\n"
"&lt;/qandaset&gt;\n"
"&lt;/article&gt;"
