"&lt;!ENTITY % procedure.role.attrib \"Role (Required|Optional) #IMPLIED\"&gt;\n"
"&lt;!-- load DocBook --&gt;\n"
"&lt;!ENTITY % DocBookDTD PUBLIC \"-//OASIS//DTD DocBook V3.1//EN\"&gt;\n"
"%DocBookDTD;<indexterm startref=\"DocBookcustomch05\" class=\"endofrange\"></indexterm><indexterm startref=\"customDocBookch05\" class=\"endofrange\"></indexterm>"
msgstr ""
"&lt;!ENTITY % procedure.role.attrib \"Role (Required|Optional) #IMPLIED\"&gt;\n"
"&lt;!-- load DocBook --&gt;\n"
"&lt;!ENTITY % DocBookDTD PUBLIC \"-//OASIS//DTD DocBook V3.1//EN\"&gt;\n"
"%DocBookDTD;<indexterm startref=\"DocBookcustomch05\" class=\"endofrange\"></indexterm><indexterm startref=\"customDocBookch05\" class=\"endofrange\"></indexterm>"

