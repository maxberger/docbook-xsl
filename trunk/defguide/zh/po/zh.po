msgid ""
"<indexterm><primary>CSS stylesheets</primary><secondary>example</secondary></"
"indexterm><acronym>CSS</acronym> stylesheets consist of selectors and "
"formatting properties, as shown in <xref linkend=\"ex-css\"/>."
msgstr ""

#. (title)
#: ../source/defguide.xml:5453
msgid "A Fragment of a <acronym>CSS</acronym> Stylesheet"
msgstr "<acronym>CSS</acronym> 样式表片断"

#. (title)
#: ../source/defguide.xml:5464
