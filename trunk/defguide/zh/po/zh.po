msgid ""
"<indexterm><primary>types (attributes)</primary></"
"indexterm><indexterm><primary>attributes</primary><secondary>types</"
"secondary></indexterm><acronym>SGML</acronym> offers a small selection of "
"attribute types. (<acronym>XML</acronym> offers a subset of these.) For "
"convenience, a brief description of each of these types is provided here:"
msgstr ""

#. (para)
#: ../source/defguide.xml:10626
msgid ""
"<indexterm><primary>characters</primary><secondary>strings (CDATA attribute)</"
"secondary></indexterm><indexterm><primary>strings (characters)</primary></"
"indexterm><indexterm><primary>CDATA</primary></indexterm> A string of "
"characters."
msgstr ""

#. (tag)
#: ../source/defguide.xml:10633
msgid "NUMBER"
msgstr "NUMBER"

#. (para)
#: ../source/defguide.xml:10636
msgid ""
"<indexterm><primary>numbers (attributes)</primary></"
"indexterm><indexterm><primary>hyphens (-), numbers</primary></"
"indexterm><indexterm><primary>decimal points (.), numbers</primary></"
"indexterm> A number. Numbers must begin with a hyphen or digit and can "
"include the decimal point."
msgstr ""

#. (tag), (entry)
#: ../source/defguide.xml:10644 ../source/defguide.xml:13266
#: ../source/defguide.xml:13531 ../source/defguide.xml:13725
msgid "NMTOKEN"
msgstr "NMTOKEN"

#. (para)
#: ../source/defguide.xml:10647
msgid ""
"<indexterm><primary>NMTOKEN(S) attribute</primary></"
"indexterm><indexterm><primary>names</primary><secondary>NMTOKEN attribute</"
"secondary></indexterm><indexterm><primary>attributes</"
"primary><secondary>names</secondary></"
"indexterm><indexterm><primary>characters</primary><secondary>names</"
"secondary></indexterm> A sequence of name characters (letters, digits, "
"hyphens, and periods). This differs from a <tag class=\"attribute\">CDATA</"
"tag> attribute because it cannot contain spaces, punctuation, or other non-"
"name characters."
msgstr ""

#. (para)
#: ../source/defguide.xml:10660
msgid ""
"A sequence of one or more space-delimited <tag class=\"attribute\">NMTOKEN</"
"tag> values."
msgstr ""

#. (tag)
#: ../source/defguide.xml:10665
msgid "NUTOKEN"
msgstr "NUTOKEN"

#. (para)
#: ../source/defguide.xml:10668
msgid ""
"<indexterm><primary>NUTOKEN attribute</primary></"
"indexterm><indexterm><primary>digits</primary><secondary>numbers</secondary></"
"indexterm><indexterm><primary>decimal points (.), numbers</primary></"
"indexterm> A sequence of number characters (digits, hyphens, and periods). "
"This differs from a <tag class=\"attribute\">NUMBER</tag> field because it is "
"not required to begin with a digit or a hyphen."
msgstr ""

#. (tag), (entry)
#: ../source/defguide.xml:10678 ../source/defguide.xml:14675
#: ../source/defguide.xml:35241 ../source/defguide.xml:37948
msgid "ENTITY"
msgstr "ENTITY"

#. (para)
#: ../source/defguide.xml:10681
msgid ""
"<indexterm><primary>ENTITY attribute</primary></indexterm> An entity name. "
"The value of an <tag class=\"attribute\">ENTITY</tag> attribute must be the "
"name of a declared entity."
msgstr ""

#. (tag)
#: ../source/defguide.xml:10689
msgid "NAME"
msgstr "NAME"

#. (para)
#: ../source/defguide.xml:10692
msgid ""
"<indexterm><primary>NAME attribute</primary></"
"indexterm><indexterm><primary>digits</primary><secondary>names</secondary></"
"indexterm><indexterm><primary>periods (.), names</primary></"
"indexterm><indexterm><primary>NAMES attribute</primary></indexterm> A name. A "
"name must begin with a letter and can consist of letters, digits, hyphens, "
"and periods."
msgstr ""

#. (tag)
#: ../source/defguide.xml:10700
msgid "NAMES"
msgstr "NAMES"

#. (para)
#: ../source/defguide.xml:10703
msgid ""
"A sequence of one or more space-delimited <tag class=\"attribute\">NAME</tag> "
"values."
msgstr ""

#. (para)
#: ../source/defguide.xml:10711
msgid ""
"<indexterm><primary>ID attribute</primary></"
"indexterm><indexterm><primary>uniqueness</primary><secondary>ID attributes</"
"secondary></indexterm> An <acronym>ID</acronym>. <acronym>ID</acronym>s are "
"names that must be globally unique within the document. The <tag class="
"\"attribute\">ID</tag> attribute declares the <acronym>ID</acronym>."
msgstr ""

#. (tag), (entry)
#: ../source/defguide.xml:10720 ../source/defguide.xml:14094
#: ../source/defguide.xml:17451 ../source/defguide.xml:17472
msgid "IDREF"
msgstr "IDREF"

#. (para)
#: ../source/defguide.xml:10723
msgid ""
"<indexterm><primary>IDREF attribute</primary></indexterm> An single "
"<acronym>ID</acronym>. <acronym>ID</acronym>s are names that must be globally "
"unique within the document. The value of an <tag class=\"attribute\">IDREF</"
"tag> attribute must be the name of an <tag class=\"attribute\">ID</tag> "
"attribute defined in the document."
msgstr ""

#. (tag), (entry)
#: ../source/defguide.xml:10734 ../source/defguide.xml:13295
#: ../source/defguide.xml:18795 ../source/defguide.xml:19234
msgid "IDREFS"
msgstr "IDREFS"

#. (para)
#: ../source/defguide.xml:10737
msgid ""
"<indexterm><primary>IDREFS attribute</primary></indexterm> A sequence of one "
