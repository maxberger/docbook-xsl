#. (refpurpose), (entry)
#: ../source/defguide.xml:63281 ../source/defguide.xml:168098
msgid "A row in a table"
msgstr "表格中的行"

#. (synopsis)
#: ../source/defguide.xml:63297
#, no-wrap
msgid ""
"row ::=\n"
"((<placeholder-1/>|<placeholder-2/>)+)"
msgstr ""

#. (tag)
