        <parameter>tex.math.delims</parameter> 
      </group>   
      <group name ="Equation">
        <attribute-set>equation.properties</attribute-set>
        <attribute-set>informalequation.properties</attribute-set>
      </group>          
    </subsection>        
    <subsection name="Index Generation">
      <group name ="Generate index">
        <parameter>generate.index</parameter>
        <parameter>make.index.markup</parameter>
        <parameter>index.on.type</parameter>
        <parameter>index.on.role</parameter>
        <parameter>index.method</parameter>
