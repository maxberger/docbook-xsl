    <subsection name="Graphics">
      <group>
        <parameter>use.svg</parameter>
        <parameter>graphic.default.extension</parameter>
        <parameter>img.src.path</parameter>
        <parameter>keep.relative.image.uris</parameter>
      </group>
      <group name ="Graphics size">
