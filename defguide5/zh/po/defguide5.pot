#: ../../en/build/defguide5.xml:65535
msgid "A titled division in a QandASet"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "qandadiv <phrase role=\"cceq\">::=</phrase>"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "A <tag>qandadiv</tag> is a section of a <tag>qandaset</tag>. A question and answer set might be divided into sections in order to group different sets of questions together, perhaps by topic."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "A <tag>qandaset</tag> may contain any number of <tag>qandadiv</tag> or <tag>qandaentry</tag> elements, but it cannot contain a mixture of both at the same level."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "A table of contents for the question and answer set is sometimes generated, especially in online environments."
msgstr ""

