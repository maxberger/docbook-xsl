msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "refmiscinfo <phrase role=\"cceq\">::=</phrase>"
msgstr ""

#.(para), (entry)
#: ../../en/build/defguide5.xml:65535 ../../en/build/defguide5.xml:65535
msgid "“source”"
msgstr ""

#.(para), (entry)
#: ../../en/build/defguide5.xml:65535 ../../en/build/defguide5.xml:65535
