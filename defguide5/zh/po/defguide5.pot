msgstr ""

#.(refmiscinfo)
#: ../../en/build/defguide5.xml:65535
msgid "db.question"
msgstr ""

#.(refpurpose)
#: ../../en/build/defguide5.xml:65535
msgid "A question in a QandASet"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "question <phrase role=\"cceq\">::=</phrase>"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "A <tag>question</tag> in a <tag>qandaentry</tag> poses a question or states a problem that is addressed by the following <tag>answer</tag>(s). The <tag>answer</tag>s are optional (some questions have no answers) and may be repeated (some questions have more than one answer)."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "The <tag>question</tag>s are frequently introduced with a label, such as <quote>A:</quote>. If a <tag>question</tag> has a <literal>Label</literal> child element, the content of that <tag>label</tag> is used as the label for the <tag>question</tag>. The <tag class=\"attribute\">defaultlabel</tag> attribute on the nearest ancestor <tag>qandaset</tag> of a <tag>question</tag> can be used to indicate that a processing application should automatically generate a label for the <tag>question</tag>."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "These elements contain <tag>question</tag>: <placeholder-1/>."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "The following elements occur in question: <placeholder-1/>."
msgstr ""

#.(refmiscinfo)
#: ../../en/build/defguide5.xml:65535
msgid "db.quote"
msgstr ""

#.(refpurpose)
#: ../../en/build/defguide5.xml:65535
msgid "An inline quotation"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "quote <phrase role=\"cceq\">::=</phrase>"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "A <tag>quote</tag> surrounds an inline quotation. Using an element for quotations is sometimes more convenient than entering the quotation marks directly, and makes it possible for a presentation system to alter the format of the quotation marks."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "Block quotations are properly identified as <tag>blockquote</tag>s."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "Formatted inline. The <tag>quote</tag> element is expected to generate the proper quotation marks. These may be influenced by the <tag class=\"attribute\">lang</tag> attribute on an ancestor element. For example, a quote in French might use «guillments» instead of English “quote marks.”"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "These elements contain <tag>quote</tag>: <placeholder-1/>."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "The following elements occur in quote: <placeholder-1/>."
msgstr ""

