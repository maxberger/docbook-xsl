msgid "A <tag>refmeta</tag> holds the title of the reference page, the number of the volume in which this reference page occurs, and possibly other miscellaneous information (typically used in printing the reference page)."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "Suppressed. Most of the elements contained in <tag>refmeta</tag> are used in presentation, but they are not generally printed as part of the formatting of the <tag>refmeta</tag> wrapper--it merely serves to identify where they occur."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "These elements contain <tag>refmeta</tag>: <placeholder-1/>."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
