#.(refmiscinfo)
#: ../../en/build/defguide5.xml:65535
msgid "db.qandaset"
msgstr ""

#.(refpurpose)
#: ../../en/build/defguide5.xml:65535
msgid "A question-and-answer set"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "qandaset <phrase role=\"cceq\">::=</phrase>"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "defaultlabel (enumeration)"
msgstr ""

#.(para), (entry)
#: ../../en/build/defguide5.xml:65535 ../../en/build/defguide5.xml:65535
msgid "“number”"
msgstr ""

#.(para), (entry)
#: ../../en/build/defguide5.xml:65535 ../../en/build/defguide5.xml:65535
msgid "“qanda”"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "A <tag>qandaset</tag> is a list consisting of <tag>question</tag>s and <tag>answer</tag>s. A <tag>qandaset</tag> can be divided into sections."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "Every entry in a <tag>qandaset</tag> must contain a <tag>question</tag>, but <tag>answer</tag>s are optional (some questions have no answers), and may be repeated (some questions have more than one answer)."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "Common uses for <tag>qandaset</tag>s include reader questionnaires and lists of <quote>Frequently Asked Questions</quote> (<acronym>FAQ</acronym>s). For the purpose of an <acronym>FAQ</acronym>, DocBook <acronym>V3.1</acronym> added the <literal>FAQ</literal> class to <tag>article</tag>."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "Formatted as a displayed block. The <tag class=\"attribute\">defaultlabel</tag> attribute has a significant influence on the presentation of <tag>question</tag>s and <tag>answer</tag>s."
msgstr ""

#.(term)
#: ../../en/build/defguide5.xml:65535
msgid "defaultlabel"
msgstr ""

