msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "A <tag>refsect3</tag> is a third level section in a <tag>refentry</tag>, analogous to a <tag>sect3</tag> elsewhere in the document. See <tag>refsect1</tag>."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "In DocBook, <tag>refsect3</tag> is the lowest-level section allowed in a <tag>refentry</tag>. There is no element analogous to a <tag>sect4</tag>."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "These elements contain <tag>refsect3</tag>: <placeholder-1/>."
msgstr ""

#.(para)
