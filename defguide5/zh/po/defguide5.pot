msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "refsection <phrase role=\"cceq\">::=</phrase>"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "Reference pages have their own hierarchical structure. A <tag>refsection</tag> is a recursive division in a <tag>refentry</tag>, analagous to a <tag>section</tag> elsewhere in the document."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "The value of a separate hierarchical structure is that it allows the content model of sections in reference pages to be customized differently than the content model of sections outside."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "These elements contain <tag>refsection</tag>: <placeholder-1/>."
