msgid "db.parameter"
msgstr ""

#.(refpurpose)
#: ../../en/build/defguide5.xml:65535
msgid "A value or a symbolic reference to a value"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "parameter <phrase role=\"cceq\">::=</phrase>"
msgstr ""

#.(para), (entry)
