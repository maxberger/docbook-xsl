msgstr ""

#.(para), (entry)
#: ../../en/build/defguide5.xml:65535 ../../en/build/defguide5.xml:65535
msgid "“inherit”"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "numeration (enumeration)"
msgstr ""

#.(para), (entry)
#: ../../en/build/defguide5.xml:65535 ../../en/build/defguide5.xml:65535
msgid "“arabic”"
msgstr ""

#.(para), (entry)
#: ../../en/build/defguide5.xml:65535 ../../en/build/defguide5.xml:65535
msgid "“upperalpha”"
msgstr ""

#.(para), (entry)
#: ../../en/build/defguide5.xml:65535 ../../en/build/defguide5.xml:65535
msgid "“loweralpha”"
msgstr ""

#.(para), (entry)
#: ../../en/build/defguide5.xml:65535 ../../en/build/defguide5.xml:65535
msgid "“upperroman”"
msgstr ""

#.(para), (entry)
#: ../../en/build/defguide5.xml:65535 ../../en/build/defguide5.xml:65535
msgid "“lowerroman”"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "In an <tag>orderedlist</tag>, each member of the list is marked with a numeral, letter, or other sequential symbol (such as roman numerals)."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "The <tag class=\"attribute\">numeration</tag> attribute indicates the desired numeration. If it is not specified, Arabic numerals (1, 2, 3, …) are to be used."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "The actually characters used in alphabetic numeration may be locale sensitive. Where the sequence “a”, “b”, “c” might be selected in an English locale, “ア”, “イ”, “ウ” might be selected if Katakana was implied by the current locale."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "The <tag class=\"attribute\">continuation</tag> attribute indicates how numbering should begin relative to the immediately preceding list. If it is not specified, numbering is to be restarted at 1."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "The preceding list is not required to be in the same parent. That is, a list in one chapter may be continued in the next, or indeed arbitrarily later in the document, provided no other list intervenes."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "The <tag class=\"attribute\">inheritnum</tag> attribute indicates how items in nested lists should be numbered. If it is not specified, numbering is to ignore list nesting. Inherited numeration creates compound list item numbers."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "If <tag class=\"attribute\">inheritnum</tag> is “<tag class=\"attvalue\">inherit</tag>”, then the third item of a list inside the second item of a list inside the fourth item of a list might be enumerated as “4.2.3”. If <tag class=\"attribute\">inheritnum</tag> is “<tag class=\"attvalue\">ignore</tag>”, then it would be simply “3”. (With the <tag class=\"attribute\">numeration</tag> attribute handling the actual format of the numbers.)"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "In nested lists, some publishers prefer to use Arabic numbers throughout (4.3.2) while others prefer to step through a sequence of numerations (4.b.iii). DocBook does not specify the sequence of numerations."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "Indicates how list numbering should begin relative to the immediately preceding list"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "Specifies that numbering should begin where the preceding list left off"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "Specifies that numbering should begin again at 1"
msgstr ""

#.(term)
#: ../../en/build/defguide5.xml:65535
msgid "inheritnum"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "Indicates whether or not item numbering should be influenced by list nesting"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "Specifies that numbering should ignore list nesting"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "Specifies that numbering should inherit from outer-level lists"
msgstr ""

#.(term)
#: ../../en/build/defguide5.xml:65535
msgid "numeration"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "Indicates the desired numeration"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "Specifies Arabic numeration (1, 2, 3, …)"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "Specifies upper-case alphabetic numeration (A, B, C, …)"
msgstr ""

