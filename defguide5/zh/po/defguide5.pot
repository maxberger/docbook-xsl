#: ../../en/build/defguide5.xml:65535 ../../en/build/defguide5.xml:65535
msgid "“sectdesc”"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "The <tag>refmiscinfo</tag> element is an escape hatch for additional meta-information about a reference page. It may hold copyright information, release or revision information, descriptive text for use in a print header or footer, or any other information not explicitly provided for in <tag>refmeta</tag>."
msgstr ""

