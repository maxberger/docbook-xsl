msgid "Information about a particular release of a document"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "releaseinfo <phrase role=\"cceq\">::=</phrase>"
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "The <tag>releaseinfo</tag> element contains a brief description of the release or published version of a document or part of a document."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "For example, the release information may state that the document is in beta, or that the software it describes is a beta version. It may also contain more specific information, such as the version number from a revision control system."
msgstr ""

#.(para)
#: ../../en/build/defguide5.xml:65535
msgid "These elements contain <tag>releaseinfo</tag>: <placeholder-1/>."
msgstr ""

