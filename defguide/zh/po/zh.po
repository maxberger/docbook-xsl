#. (title)
#: ../source/defguide.xml:5677
msgid "Publishing DocBook Documents"
msgstr "出版 DocBook 文档"

#. (para)
#: ../source/defguide.xml:5678
#, fuzzy
msgid ""
"<indexterm id=\"publishDocBookch04\" class=\"startofrange\" significance="
"\"normal\"><primary>DocBook DTD</primary><secondary>publishing documents</"
"secondary></indexterm><indexterm id=\"DocBookpublishch04\" class="
"\"startofrange\" significance=\"normal\"><primary>publishing DocBook "
