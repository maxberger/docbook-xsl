#. (para)
#: ../source/defguide.xml:169938
#, fuzzy
msgid "DocBook in <filename moreinfo=\"none\">/share/sgml/docbk30/</filename>,"
msgstr "DocBook 在 <filename>/share/sgml/docbk30/</filename> 中,"

#. (para)
#: ../source/defguide.xml:169941
#, fuzzy
msgid ""
"The ISO entities in <filename moreinfo=\"none\">/share/sgml/entities/8879/</"
"filename>, and"
msgstr "ISO 实体在 <filename>/share/sgml/entities/8879/</filename> 中，以及"

#. (para)
#: ../source/defguide.xml:169945
#, fuzzy
msgid ""
"Your catalog in <filename moreinfo=\"none\">/share/sgml/catalog</filename>"
msgstr "你的编目在 <filename>/share/sgml/catalog</filename> 中"

#. (para)
#: ../source/defguide.xml:169948
