#. (literal)
#: ../source/defguide.xml:7321
msgid "%gentext-language%"
msgstr "%gentext-language%"

#. (para)
#: ../source/defguide.xml:7321
msgid ""
"If a language code is specified in <literal moreinfo=\"none\">%gentext-"
"language%</literal>, then that language will be used for all generated text, "
"regardless of any <sgmltag class=\"attribute\">lang</sgmltag> attribute "
"settings in the document."
msgstr ""

#. (literal)
#: ../source/defguide.xml:7326
