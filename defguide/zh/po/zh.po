msgid "Customizing the Stylesheets"
msgstr "定制样式表"

#. (para)
#: ../source/defguide.xml:6801
msgid ""
"<indexterm significance=\"normal\"><primary>DSSSL</"
"primary><secondary>stylesheets</secondary><tertiary>customizing</tertiary></"
"indexterm><indexterm significance=\"normal\"><primary>customizing</"
"primary><secondary>DSSSL stylesheets</secondary></indexterm><indexterm "
"significance=\"normal\"><primary>driver files</primary><secondary>DSSSL "
