"\n"
"\n"
"&lt;!ENTITY % dbpool.redecl.module \"INCLUDE\"&gt;\n"
"&lt;!ENTITY % rdbpool SYSTEM \"rdbpool.mod\"&gt;\n"
"\n"
"&lt;!-- load DocBook --&gt;\n"
"&lt;!ENTITY % DocBookDTD PUBLIC \"-//OASIS//DTD DocBook V3.1//EN\"&gt;\n"
