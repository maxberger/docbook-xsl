msgstr "方程式"

#. (para)
#: ../source/defguide.xml:3541
#, fuzzy
msgid ""
"<indexterm significance=\"normal\"><primary>Equation element</primary></"
"indexterm><indexterm significance=\"normal\"><primary>titles</"
"primary><secondary>equation elements</secondary></indexterm><indexterm "
"significance=\"normal\"><primary>InlineEquation element</primary></indexterm> "
