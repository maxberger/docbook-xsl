#.(para)
#: ../source/defguide.xml:38435
msgid "<tag class=\"attribute\">Depth</tag> specifies the desired depth of the viewport area."
msgstr ""

#.(para)
#: ../source/defguide.xml:38443
msgid "<tag class=\"attribute\">EntityRef</tag> identifies the general entity which points to the content of the image data."
msgstr ""

#.(para)
#: ../source/defguide.xml:38451
msgid "<tag class=\"attribute\">FileRef</tag> specifies the name of the file which contains the content of the image data."
