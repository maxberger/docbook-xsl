msgid "<tag class=\"attribute\">Align</tag> specifies the horizontal alignment of the content area in the viewport area."
msgstr ""

#.(para)
#: ../source/defguide.xml:38419
msgid "<tag class=\"attribute\">ContentDepth</tag> specifies the desired depth of the content area."
msgstr ""

