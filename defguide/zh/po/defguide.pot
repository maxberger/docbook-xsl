msgstr ""

#.(refpurpose), (entry)
#: ../source/defguide.xml:60402 ../source/defguide.xml:65535
msgid "The name, purpose, and classification of a reference page"
msgstr ""

#.(synopsis)
#: ../source/defguide.xml:60418
#, no-wrap
msgid "refnamediv ::=\n(<placeholder-1/>?,<placeholder-2/>+,<placeholder-3/>,<placeholder-4/>*,\n (<placeholder-5/>|<placeholder-6/>|<placeholder-7/>|<placeholder-8/>)*)"
msgstr ""

#.(para)
#: ../source/defguide.xml:60439
msgid "<tag>RefNameDiv</tag> is the first mandatory section in a <tag>RefEntry</tag>. It is a peer to <tag>RefSynopsisDiv</tag> and <tag>RefSect1</tag>."
msgstr ""

#.(para)
#: ../source/defguide.xml:60443
msgid "The elements in <tag>RefNameDiv</tag> identify the topic of the reference page (<tag>RefDescriptor</tag> or <tag>RefName</tag>), provide a concise summary (<tag>RefPurpose</tag>), and classify the page (<tag>RefClass</tag>)."
msgstr ""

#.(para)
#: ../source/defguide.xml:60453
msgid "Formatted as a displayed block. <tag>RefNameDiv</tag> usually generates a section heading, in the same typographic style as a <tag>RefSect1</tag><tag>Title</tag>, called <quote>Name.</quote>"
msgstr ""

#.(para)
#: ../source/defguide.xml:60458
msgid "The content of this section is traditionally the <tag>RefDescriptor</tag> or <tag>RefName</tag>, and the <tag>RefPurpose</tag>, separated by an em dash."
msgstr ""

#.(para)
#: ../source/defguide.xml:60463
msgid "The <tag>RefClass</tag> may be presented, or it may be suppressed and used only to select a group of reference pages to process. You might use the value of <tag>RefClass</tag> to print all the reference pages appropriate to <productname>Solaris</productname><acronym>UNIX</acronym>, for example."
msgstr ""

#.(para)
#: ../source/defguide.xml:60486
msgid "These elements contain refnamediv: <placeholder-1/>."
