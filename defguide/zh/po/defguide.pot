#.(para)
#: ../source/defguide.xml:28816
msgid "<tag class=\"attribute\">Cols</tag> specifies the number of columns in the <tag>EntryTbl</tag>."
msgstr ""

#.(para)
#: ../source/defguide.xml:28824
