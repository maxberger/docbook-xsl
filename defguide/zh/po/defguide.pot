#: ../source/defguide.xml:38237
msgid "<placeholder-1/> default"
msgstr ""

#.(entry)
#: ../source/defguide.xml:38242 ../source/defguide.xml:38243 ../source/defguide.xml:38248 ../source/defguide.xml:38252
msgid "unspecified"
msgstr ""

#.(entry)
#: ../source/defguide.xml:38244
msgid "irrelevant"
msgstr ""

