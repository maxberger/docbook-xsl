      <oneOrMore>
        <choice>
          <ref name="book.class"/>
        </choice>
      </oneOrMore>
      <optional>
        <ref name="setindex.element"/>
      </optional>
    </group>
  </element>
</define>
<dtd:marked-section end="set.element"/>

