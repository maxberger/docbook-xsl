  </optional>
</define>

<define name="bookcomponent.title.content">
  <ref name="title"/>
  <optional>
    <ref name="subtitle"/>
  </optional>
  <optional>
    <ref name="titleabbrev"/>
  </optional>
</define>

<define name="sect.title.content">
