<define name="local.notation.class">
  <notAllowed/>
</define>

<define name="notation.class">
  <choice>
    <value>BMP</value>
    <value>CGM-CHAR</value>
    <value>CGM-BINARY</value>
    <value>CGM-CLEAR</value>
    <value>DITROFF</value>
    <value>DVI</value>
    <value>EPS</value>
    <value>EQN</value>
    <value>FAX</value>
    <value>GIF</value>
    <value>GIF87a</value>
    <value>GIF89a</value>
    <value>JPG</value>
    <value>JPEG</value>
    <value>IGES</value>
    <value>PCX</value>
    <value>PIC</value>
    <value>PNG</value>
    <value>PS</value>
    <value>SGML</value>
    <value>TBL</value>
    <value>TEX</value>
    <value>TIFF</value>
    <value>WMF</value>
    <value>WPG</value>
    <value>linespecific</value>
    <ref name="local.notation.class"/>
  </choice>
</define>

