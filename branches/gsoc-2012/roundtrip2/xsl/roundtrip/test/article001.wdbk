    <dbk:blockquote>
      <dbk:attribution>Ogden Nash</dbk:attribution>
      <dbk:para>Candy is dandy,</dbk:para>
      <dbk:para>but liquor is quicker!</dbk:para>
    </dbk:blockquote>
    <sect2>
